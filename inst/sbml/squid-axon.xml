    <listOfParameters>
      <parameter metaid="metaid_0000010" id="V" name="transmembrane voltage" value="-75" units="millivolt" constant="false"/>
      <parameter metaid="metaid_0000011" id="I" name="applied current" value="0" units="microA_per_cm2"/>
      <parameter metaid="metaid_0000012" id="i_Na" name="sodium current" units="microA_per_cm2" constant="false"/>
      <parameter metaid="metaid_0000013" id="i_K" name="potassium current" units="microA_per_cm2" constant="false"/>
      <parameter metaid="metaid_0000014" id="i_L" name="leakage current" units="microA_per_cm2" constant="false"/>
      <parameter metaid="metaid_0000015" id="m" name="sodium channel activation coefficient" value="0.05" units="dimensionless" constant="false"/>
      <parameter metaid="metaid_0000016" id="h" name="sodium channel inactivation coefficient" value="0.6" units="dimensionless" constant="false"/>
      <parameter metaid="metaid_0000017" id="n" name="potassium channel activation coefficient" value="0.325" units="dimensionless" constant="false"/>
      <parameter metaid="metaid_0000018" id="E_R" name="resting membrane potential" value="-75" units="millivolt"/>
      <parameter metaid="metaid_0000019" id="Cm" name="membrane capacitance" value="1" units="microF_per_cm2"/>
      <parameter metaid="metaid_0000020" id="g_Na" name="maximum sodium channel conductance" value="120" units="milliS_per_cm2"/>
      <parameter metaid="metaid_0000021" id="g_K" name="maximum potassium channel conductance" value="36" units="milliS_per_cm2"/>
      <parameter metaid="metaid_0000022" id="g_L" name="maximum leakage conductance" value="0.3" units="milliS_per_cm2"/>
      <parameter metaid="metaid_0000023" id="E_Na" name="sodium equilibrium potential" units="millivolt" constant="false"/>
      <parameter metaid="metaid_0000024" id="E_K" name="potassium equilibrium potential" units="millivolt" constant="false"/>
      <parameter metaid="metaid_0000025" id="E_L" name="leakage equilibrium potential" units="millivolt" constant="false"/>
      <parameter metaid="metaid_0000026" id="alpha_m" name="auxiliary alpha_m" units="per_millisecond" constant="false"/>
      <parameter metaid="metaid_0000027" id="beta_m" name="auxiliary beta_m" units="per_millisecond" constant="false"/>
      <parameter metaid="metaid_0000028" id="alpha_h" name="auxiliary alpha_h" units="per_millisecond" constant="false"/>
      <parameter metaid="metaid_0000029" id="beta_h" name="auxiliary beta_h" units="per_millisecond" constant="false"/>
      <parameter metaid="metaid_0000030" id="alpha_n" name="auxiliary alpha_n" units="per_millisecond" constant="false"/>
      <parameter metaid="metaid_0000031" id="beta_n" name="auxiliary beta_n" units="per_millisecond" constant="false"/>
    </listOfParameters>
    <listOfRules>
      <assignmentRule metaid="metaid_0000033" variable="E_Na">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				E_Na = E_R + 115.0
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <ci> E_R </ci>
            <cn> 115 </cn>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000034" variable="E_K">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				E_K = E_R + 12.0
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <ci> E_R </ci>
            <cn> 12 </cn>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000035" variable="E_L">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				E_L = E_R + 10.613
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <ci> E_R </ci>
            <cn> 10.613 </cn>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000036" variable="alpha_m">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				alpha_m = 0.1 * (V + 25.0)/(exp[0.1 * (V + 25.0)] - 1.0)
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <apply>
              <times/>
              <cn> 0.1 </cn>
              <apply>
                <plus/>
                <ci> V </ci>
                <cn> 25 </cn>
              </apply>
            </apply>
            <apply>
              <minus/>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <cn> 0.1 </cn>
                  <apply>
                    <plus/>
                    <ci> V </ci>
                    <cn> 25 </cn>
                  </apply>
                </apply>
              </apply>
              <cn> 1 </cn>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000037" variable="beta_m">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				beta_m = 4.0 * exp[V / 18.0]
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn> 4 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <ci> V </ci>
                <cn> 18 </cn>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000038" variable="alpha_h">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				alpha_h = 0.07 * exp[V / 20.0]
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn> 0.07 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <ci> V </ci>
                <cn> 20 </cn>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000039" variable="beta_h">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				beta_h = 1.0 /(exp[0.1 * (V + 30.0)] + 1.0)
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <cn> 1 </cn>
            <apply>
              <plus/>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <cn> 0.1 </cn>
                  <apply>
                    <plus/>
                    <ci> V </ci>
                    <cn> 30 </cn>
                  </apply>
                </apply>
              </apply>
              <cn> 1 </cn>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000040" variable="alpha_n">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				alpha_n = 0.01 * (V + 10.0)/(exp[0.1 * (V + 10.0)] - 1.0)
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <apply>
              <times/>
              <cn> 0.01 </cn>
              <apply>
                <plus/>
                <ci> V </ci>
                <cn> 10 </cn>
              </apply>
            </apply>
            <apply>
              <minus/>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <cn> 0.1 </cn>
                  <apply>
                    <plus/>
                    <ci> V </ci>
                    <cn> 10 </cn>
                  </apply>
                </apply>
              </apply>
              <cn> 1 </cn>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000041" variable="beta_n">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				beta_n = 0.125 * exp[V / 80.0]
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn> 0.125 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <ci> V </ci>
                <cn> 80 </cn>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <rateRule metaid="metaid_0000045" variable="m">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				dm/dt = alpha_m * (1.0 - m) - beta_m * m
			</p>
        </notes>
        <annotation>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/" >
<rdf:Description rdf:about="#metaid_0000045">
<bqbiol:isPartOf>
<rdf:Bag>
<rdf:li rdf:resource="http://www.geneontology.org/#GO:0005248"/>
</rdf:Bag>
</bqbiol:isPartOf>
</rdf:Description>
</rdf:RDF>
</annotation>

        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> alpha_m </ci>
              <apply>
                <minus/>
                <cn> 1 </cn>
                <ci> m </ci>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> beta_m </ci>
              <ci> m </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <rateRule metaid="metaid_0000046" variable="h">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				dh/dt = alpha_h * (1.0 - h) - beta_h * h
			</p>
        </notes>
        <annotation>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/" >
<rdf:Description rdf:about="#metaid_0000046">
<bqbiol:isPartOf>
<rdf:Bag>
<rdf:li rdf:resource="http://www.geneontology.org/#GO:0005248"/>
</rdf:Bag>
</bqbiol:isPartOf>
</rdf:Description>
</rdf:RDF>
</annotation>

        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> alpha_h </ci>
              <apply>
                <minus/>
                <cn> 1 </cn>
                <ci> h </ci>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> beta_h </ci>
              <ci> h </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <rateRule metaid="metaid_0000047" variable="n">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				dn/dt = alpha_n * (1.0 - n) - beta_n * n
			</p>
        </notes>
        <annotation>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/" >
<rdf:Description rdf:about="#metaid_0000047">
<bqbiol:isPartOf>
<rdf:Bag>
<rdf:li rdf:resource="http://www.geneontology.org/#GO:0005249"/>
</rdf:Bag>
</bqbiol:isPartOf>
</rdf:Description>
</rdf:RDF>
</annotation>

        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> alpha_n </ci>
              <apply>
                <minus/>
                <cn> 1 </cn>
                <ci> n </ci>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> beta_n </ci>
              <ci> n </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <assignmentRule metaid="metaid_0000042" variable="i_Na">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				i_Na = g_Na * m^3.0 * h * (V - E_Na)
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> g_Na </ci>
            <apply>
              <power/>
              <ci> m </ci>
              <cn> 3 </cn>
            </apply>
            <ci> h </ci>
            <apply>
              <minus/>
              <ci> V </ci>
              <ci> E_Na </ci>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000043" variable="i_K">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				i_K = g_K * n^4.0 * (V - E_K)
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> g_K </ci>
            <apply>
              <power/>
              <ci> n </ci>
              <cn> 4 </cn>
            </apply>
            <apply>
              <minus/>
              <ci> V </ci>
              <ci> E_K </ci>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000044" variable="i_L">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				i_L = g_L * (V - E_L)
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> g_L </ci>
            <apply>
              <minus/>
              <ci> V </ci>
              <ci> E_L </ci>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <rateRule metaid="metaid_0000048" variable="V">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
				dV/dt = (I - (i_Na + i_K + i_L))/Cm
			</p>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <apply>
              <minus/>
              <ci> I </ci>
              <apply>
                <plus/>
                <ci> i_Na </ci>
                <ci> i_K </ci>
                <ci> i_L </ci>
              </apply>
            </apply>
            <ci> Cm </ci>
          </apply>
        </math>
      </rateRule>
    </listOfRules>
